issn = {0066-4308},
journal = {Annual review of psychology},
keywords = {Attention,Attention: physiology,Brain,Brain: anatomy {\&} histology,Cognition,Cognition: physiology,Humans,Memory,Neural Pathways,Neural Pathways: physiology,Psychological Theory,Psychophysiology,Retention (Psychology),Short-Term,Short-Term: physiology,Time Factors},
