@misc{Mensink1989,
abstract = {Develops a model for contextual fluctuation that is based on stimulus sampling theory (J. G. W. Raaijmakers and R. M. Shiffrin, 1980). The model assumes that "context" can be represented by a set of so-called contextual elements. Part of this set represents the current context. There is a continuous fluctuation between the active contextual elements (the current context) and the inactive contextual elements. During study, part of the current context is conditioned to (stored in) the memory trace. The model may be used to derive predictions for the overlap between the study and text contexts. The model is developed in the context of interference paradigms. It is shown that the model leads to a number of interesting predictions. The incorporation of this model in the search of associative memory theory (Raaijmakers and Shiffrin; see record 1981-20491-001) enables that theory to predict such phenomena as spontaneous recovery and proactive inhibition. (PsycINFO Database Record (c) 2002 APA, all rights reserved)},
author = {Mensink, Ger-Jan M and Raaijmakers, Jeroen G. W.},
booktitle = {Journal of Mathematical Psychology},
doi = {10.1016/0022-2496(89)90029-1},
issn = {00222496},
pages = {172--186},
title = {{A model for contextual fluctuation}},
volume = {33},
year = {1989}
}
@article{Surprenant2007,
abstract = {The present experiment investigated the hypothesis that age-related declines in cognitive functioning are partly due to a decrease in peripheral sensory functioning. In particular, it was suggested that some of the decline in serial recall for verbal material might be due to even small amounts of degradation due to noise or hearing loss. Older and younger individuals identified and recalled nonsense syllables in order at a number of different speech-to-noise ratios. Performance on the identification task was significantly correlated with performance on a subsequent serial recall task. However, this was restricted to the case in which the stimuli were presented in a substantial amount of noise. These data show that even small changes in sensory processing can lead to real and measurable declines in cognitive functioning as measured by a serial recall task.},
author = {Surprenant, Aim{\'{e}}e M},
institution = {Psychology Department, Memorial University of Newfoundland, St. John's, NL, Canada. asurpren@mun.ca},
journal = {Neuropsychology development and cognition Section B Aging neuropsychology and cognition},
keywords = {80 over,acoustic stimulation,adult,aged,aging,auditory perception,auditory perception physiology,auditory threshold,auditory threshold physiology,female,humans,identification (psychology),male,mental recall,mental recall physiology,middle aged,noise,statistics topic},
