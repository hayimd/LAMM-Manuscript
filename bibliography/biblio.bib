month = {jan},
number = {8},
pages = {e73776},
pmid = {24023690},
title = {{Reactivation in working memory: an attractor network model of free recall.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3758294{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {8},
