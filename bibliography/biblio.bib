issn = {1529-2401},
journal = {The Journal of neuroscience : the official journal of the Society for Neuroscience},
keywords = {Action Potentials,Action Potentials: physiology,Afferent Pathways,Afferent Pathways: physiology,Alpha Rhythm,Animals,Female,Fourier Analysis,Neurons,Neurons: physiology,Rats,Rats, Long-Evans,Reaction Time,Reaction Time: physiology,Somatosensory Cortex,Somatosensory Cortex: anatomy {\&} histology,Somatosensory Cortex: physiology,Taste,Taste: physiology,Theta Rhythm,Time Factors,Wakefulness,Wakefulness: physiology},
month = {mar},
