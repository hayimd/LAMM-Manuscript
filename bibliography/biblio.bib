pages = {2523--2544},
pmid = {25602775},
title = {{Scaling Laws of Associative Memory Retrieval}},
url = {http://www.mitpressjournals.org/doi/abs/10.1162/NECO{\_}a{\_}00499},
volume = {25},
year = {2013}
}
