@misc{Douglas1989a,
abstract = {We have used microanatomy derived from single neulons/ and in aiuo intracellular recordings to develop a simplified circuit of the visual cortex. The circuit explains the intracellular responses to pulse stim- ulation in tenns of the interactions between three basic populations of neurons, and reveals the following features of cortical processing that are important to computational theories of neocortex. First, inhi- bition and excitation are not separable events. Activation of the cortex inevitably sets in motion a sequence of excitation and inhibition in every neuron. Second, the thalamic input does not provide the maior excitation arriving at any neuron. Instead the intracortical excitatory connections provide most of the excitation. Third, the time evolution of excitation and inhibition is far longer than the synaptic delays of the circuits involved. This means that cortical processing cannot rely on precise timing between individual synaptic inputs.},
author = {Douglas, Rodney J. and Martin, Kevan A.C. and Whitteridge, David},
booktitle = {Neural Computation},
