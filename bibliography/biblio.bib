issn = {0340-1200},
journal = {Biological cybernetics},
keywords = {Action Potentials,Action Potentials: physiology,Animals,Learning,Learning: physiology,Mathematics,Memory,Memory: physiology,Nerve Net,Neuronal Plasticity,Neuronal Plasticity: physiology,Neurons,Neurons: cytology,Neurons: physiology,Synapses,Synapses: physiology,Time Factors,memory general theory},
