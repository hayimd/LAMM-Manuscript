keywords = {Artificial Intelligence,Bayes Theorem,Cerebral Cortex,Cerebral Cortex: physiology,Feedback,Markov Chains,Memory,Memory: physiology,Models, Neurological,Pattern Recognition, Automated,Pattern Recognition, Automated: methods,Pyramidal Cells,Pyramidal Cells: physiology},
