journal = {Nature},
keywords = {Animals,Dendritic Spines,Dendritic Spines: physiology,Dendritic Spines: ultrastructure,Male,Mice,Mice, Transgenic,Neocortex,Neocortex: cytology,Neurons,Neurons: cytology,Neurons: physiology,Synapses,Touch,Touch: physiology,Vibrissae},
