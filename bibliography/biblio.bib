keywords = {canonical microcircuits,competition,contraction theory,inhibitory plasticity,plasticity,self-organization,winner-take-all,winner-take-all, competition, plasticity, self-org},
number = {July},
pages = {68},
pmid = {25071538},
title = {{Learning and stabilization of winner-take-all dynamics through interacting excitatory and inhibitory plasticity.}},
