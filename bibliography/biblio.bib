pages = {e1000797},
pmid = {20532211},
title = {{Maturation of GABAergic inhibition promotes strengthening of temporally coherent inputs among convergent pathways.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2880567{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {6},
